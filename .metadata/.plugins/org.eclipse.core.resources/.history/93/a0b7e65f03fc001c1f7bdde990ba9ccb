package cn.zhou.inner;

public class StaticInnerClassDemo {

}

class Outer_1 {
	static int n = 1;
	static class Inner {
		static int i = 0;
		static int j = 1;
		
		public void m() {
			System.out.println(n);
			System.out.println(i);
			System.out.println();
		}
	Inner in = new Inner();
	in.m();
	}
}