package cn.zhou.inner;

public class StaticInnerClassDemo {

}

class Outer {
	static int n = 1;
	static class Inner {
		static int i = 0;
		static int j = 1;
		
		public int m() {
			System.out.println(n);
			System.out.println(i);
			System.out.println();
			return 1;
		}
	Inner in = new Inner();
	int k = in.m();
	}
}