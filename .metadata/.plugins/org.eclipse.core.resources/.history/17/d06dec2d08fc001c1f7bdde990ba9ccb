package cn.zhou.inner;

public class AnonymousInnerClassDemo {
	Calc c = new Calc(){  //利用匿名内部类创建对象
		@Override
		public double sum(double i, double j) {
			return i + j;
		}
	};
}

interface Calc {
	double sum(double i, double j);
}

