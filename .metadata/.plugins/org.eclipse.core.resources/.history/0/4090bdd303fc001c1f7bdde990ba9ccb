package cn.zhou.inner;

public class StaticInnerClassDemo {
	Outer_1.Inner oi1 = new Outer_1.new Inner();
}

class Outer_1 {
	static int n = 1;
	static class Inner_1 {
		static int i = 0;
		static int j = 1;
		
		public void m() {
			System.out.println(n);
			System.out.println(i);
			System.out.println();
		}
	Inner_1 in = new Inner_1();
//	in.m();
	}
}